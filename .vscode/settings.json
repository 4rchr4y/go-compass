{
  "rust-analyzer.showUnlinkedFileNotification": false,
  "cSpell.words": [
    "ahash",
    "bindgen",
    "canonicalize",
    "chrono",
    "filetree",
    "hashbrown",
    "inmem",
    "jsonwebtoken",
    "Keyable",
    "keymap",
    "libc",
    "mimalloc",
    "mossland",
    "mossql",
    "oneshot",
    "otlp",
    "overrider",
    "realfs",
    "repr",
    "revisioned",
    "rocksdb",
    "schemable",
    "seaorm",
    "sess",
    "smol",
    "specta",
    "sqlx",
    "surql",
    "surrealdb",
    "tablename",
    "tauri",
    "tgui",
    "thiserror",
    "unpark",
    "unparker",
    "unwatch",
    "worktree"
  ],
  "sqltools.connections": [
    {
      "previewLimit": 50,
      "driver": "SQLite",
      "name": "root",
      "group": "moss",
      "database": "${workspaceFolder:moss}/root.db"
    },
    {
      "previewLimit": 50,
      "driver": "SQLite",
      "name": "cache",
      "database": "${workspaceFolder:moss}/.moss/cache/cache.db",
      "group": "moss"
    }
  ],
  "sqltools.useNodeRuntime": true,
  "todohighlight.keywords": [
    {
      "text": "NOTE:",
      "color": "#171717",
      "backgroundColor": "#f5f5f4",
      "isWholeLine": false
    },
    {
      "text": "TODO:",
      "color": "#171717",
      "backgroundColor": "#facc15",
      "isWholeLine": false
    },
    {
      "text": "OPTIMIZE:",
      "color": "#171717",
      "backgroundColor": "#d97706",
      "isWholeLine": false
    },
    {
      "text": "BUG:",
      "color": "#171717",
      "backgroundColor": "#b91c1c",
      "isWholeLine": false
    },
    {
      "text": "FIXME:",
      "color": "#171717",
      "backgroundColor": "#f87171",
      "isWholeLine": false
    },
    {
      "text": "HACK:",
      "color": "#171717",
      "backgroundColor": "#9333ea",
      "isWholeLine": false
    }
  ],
  "files.associations": {
    "*.css": "tailwindcss"
  },
  "editor.quickSuggestions": {
    "strings": "on"
  },
  "eslint.workingDirectories": [
    {
      "mode": "auto"
    }
  ],
}